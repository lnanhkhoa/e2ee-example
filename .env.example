# .env.example

# Secret key for AES encryption (change this to a strong, random value in production)
AES_SECRET=your_aes_secret_here

# Secret key for JWT signing (change this to a strong, random value in production)
JWT_SECRET=your_jwt_secret_here

# Redis connection URL (update if using a remote Redis instance)
REDIS_URL=redis://localhost:6379/0

# KMS Key ID for key management (update as needed for your environment)
KMS_KEY_ID=your_kms_key_id_here

# Database connection URL (update as needed for your environment)
DATABASE_URL=your_database_url_here
